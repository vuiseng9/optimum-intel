{
    "algorithm": "movement_sparsity",
    "params": {
        "power": 3,
        "init_importance_threshold": -10.0,
        "final_importance_threshold": 0.0,
        "warmup_start_epoch": 1,
        "warmup_end_epoch": 10,
        "steps_per_epoch": 5533,
        "importance_regularization_factor": 0.030,
    },
    "sparse_structure_by_scopes": [
        {"mode": "block", "sparse_factors": [32, 32], "target_scopes": "{re}.*BertAttention*"},
        {"mode": "per_dim", "axis": 0, "target_scopes": "{re}.*BertIntermediate.*"},
        {"mode": "per_dim", "axis": 1, "target_scopes": "{re}.*BertOutput.*"}
    ],
    "ignored_scopes": ["{re}.*NNCFEmbedding", "{re}.*qa_outputs*", "{re}.*LayerNorm.*"]
}
