{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "optimum-nncf qat beans",
            "type": "python",
            "request": "launch",
            "env": {
                "CUDA_VISIBLE_DEVICES": "5",
                // "TORCH_DISTRIBUTED_DEBUG": "DETAIL",
                // "WANDB_MODE": "disabled",
                // "WANDB_PROJECT": "dbg_sparsity_squad",
                // "WANDB_WATCH": "false",
            },
            "cwd": "${workspaceFolder}/optimum-intel/examples/openvino/image-classification",
            "justMyCode": false,
            "program": "run_image_classification.py",
            "args": [
                "--model_name_or_path", "nateraw/vit-base-beans",
                "--dataset_name", "beans",
                "--remove_unused_columns", "False",
                "--do_eval",
                "--do_train",
                "--per_device_eval_batch_size", "8",
                "--per_device_train_batch_size", "8",
                "--learning_rate", "2e-5",
                "--num_train_epochs", "1",
                "--logging_steps", "1",
                "--overwrite_output_dir",
                "--output_dir",
                    "/tmp/vscode-runs/optimum/beans-qat",
                "--run_name", "optimum-nncf-qat-vit-beans",
                "--max_steps", "20",
            ]
        },
        {
            "name": "optimum-nncf qat squad",
            "type": "python",
            "request": "launch",
            "env": {
                "CUDA_VISIBLE_DEVICES": "5",
                // "TORCH_DISTRIBUTED_DEBUG": "DETAIL",
                // "WANDB_MODE": "disabled",
                // "WANDB_PROJECT": "dbg_sparsity_squad",
                // "WANDB_WATCH": "false",
            },
            "cwd": "${workspaceFolder}/optimum-intel/examples/openvino/question-answering",
            "justMyCode": false,
            "program": "run_qa.py",
            "args": [
                "--model_name_or_path", "distilbert-base-uncased-distilled-squad",
                "--dataset_name", "squad",
                "--do_eval",
                "--do_train",
                "--per_device_eval_batch_size", "8",
                "--per_device_train_batch_size", "8",
                "--max_seq_length", "384",
                "--doc_stride", "128",
                "--max_train_sample", "1024",
                "--learning_rate", "3e-5",
                "--num_train_epochs", "1",
                "--logging_steps", "1",
                "--overwrite_output_dir",
                "--output_dir",
                    "/tmp/vscode-runs/optimum/qa-qat",
                "--run_name", "optimum-nncf-qat-distilbert-qa",
            ]
        },
        {
            "name": "Eval squad",
            // this model has been pretrained and released by official HF
            // https://huggingface.co/bert-large-uncased-whole-word-masking-finetuned-squad
            // f1 = 93.1584
            // exact_match = 86.91
            // eval_samples = 10784
            "type": "python",
            "request": "launch",
            "env": {
                "CUDA_VISIBLE_DEVICES": "3",
                "TORCH_DISTRIBUTED_DEBUG": "DETAIL",
                "WANDB_MODE": "disabled",
            },
            "cwd": "${workspaceFolder}/optimum-intel/examples/openvino/question-answering",
            "program": "run_qa.py",
            "args": [
                "--model_name_or_path",
                "vuiseng9/bert-base-uncased-squad",
                // "csarron/bert-base-uncased-squad-v1",
                // "bert-large-uncased-whole-word-masking-finetuned-squad",
                "--dataset_name", "squad",
                "--do_eval",
                "--per_device_eval_batch_size", "128",
                "--max_seq_length", "384",
                "--doc_stride", "128",
                "--output_dir", "/tmp/vscode-runs/optimum/eval-squad1",
                "--overwrite_output_dir"
            ]
        },
    ]
}